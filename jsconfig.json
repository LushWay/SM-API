{
	"compilerOptions": {
		"alwaysStrict": true,
		"allowJs": true,
		"checkJs": true,
		"forceConsistentCasingInFileNames": true,
		"lib": ["ES2020"],
		"target": "es2020",
		"module": "es2020",
		"baseUrl": ".",
		"paths": {
			"@minecraft/*": ["node_modules/@minecraft/*/index.js"],
			"*": ["scripts/*"]
		},
		"maxNodeModuleJsDepth": 0,
		"moduleResolution": "Node16",
		"types": []
	},
	"include": ["./scripts/"],
	"exclude": ["leafs", "types"]
}
