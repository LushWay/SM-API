{
  "compilerOptions": {
    "alwaysStrict": true,
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "lib": ["es2020"],
    "target": "es2020",
    "types": [],
    "module": "ES2020",
    "moduleResolution": "Bundler",
    "baseUrl": "scripts/"
  },
  "include": ["scripts"],
  "exclude": [
    "scripts/chestui",
    "scripts/@minecraft/",
    "@minecraft/vanilla-data",
    "node_modules"
  ]
}
